{"ast":null,"code":"export default {\n  mounted() {\n    const inputField = document.querySelector('.chosen-value');\n    const dropdown = document.querySelector('.value-list');\n    const dropdownArray = [...document.querySelectorAll('li')];\n    console.log(typeof dropdownArray);\n    dropdown.classList.add('open');\n    inputField.focus(); // Demo purposes only\n\n    let valueArray = [];\n    dropdownArray.forEach(item => {\n      valueArray.push(item.textContent);\n    });\n\n    const closeDropdown = () => {\n      dropdown.classList.remove('open');\n    };\n\n    inputField.addEventListener('input', () => {\n      dropdown.classList.add('open');\n      let inputValue = inputField.value.toLowerCase();\n      let valueSubstring;\n\n      if (inputValue.length > 0) {\n        for (let j = 0; j < valueArray.length; j++) {\n          if (!(inputValue.substring(0, inputValue.length) === valueArray[j].substring(0, inputValue.length).toLowerCase())) {\n            dropdownArray[j].classList.add('closed');\n          } else {\n            dropdownArray[j].classList.remove('closed');\n          }\n        }\n      } else {\n        for (let i = 0; i < dropdownArray.length; i++) {\n          dropdownArray[i].classList.remove('closed');\n        }\n      }\n    });\n    dropdownArray.forEach(item => {\n      item.addEventListener('click', evt => {\n        inputField.value = item.textContent;\n        dropdownArray.forEach(dropdown => {\n          dropdown.classList.add('closed');\n        });\n      });\n    });\n    inputField.addEventListener('focus', () => {\n      inputField.placeholder = 'Type to filter';\n      dropdown.classList.add('open');\n      dropdownArray.forEach(dropdown => {\n        dropdown.classList.remove('closed');\n      });\n    });\n    inputField.addEventListener('blur', () => {\n      inputField.placeholder = 'Select state';\n      dropdown.classList.remove('open');\n    });\n    document.addEventListener('click', evt => {\n      const isDropdown = dropdown.contains(evt.target);\n      const isInput = inputField.contains(evt.target);\n\n      if (!isDropdown && !isInput) {\n        dropdown.classList.remove('open');\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAeI,eAAc;EACXA,OAAO,GAAE;IACd,MAAMC,UAAS,GAAIC,QAAQ,CAACC,aAAT,CAAuB,eAAvB,CAAnB;IACF,MAAMC,QAAO,GAAIF,QAAQ,CAACC,aAAT,CAAuB,aAAvB,CAAjB;IACA,MAAME,aAAY,GAAI,CAAC,GAAIH,QAAQ,CAACI,gBAAT,CAA0B,IAA1B,CAAL,CAAtB;IACAC,OAAO,CAACC,GAAR,CAAY,OAAOH,aAAnB;IACAD,QAAQ,CAACK,SAAT,CAAmBC,GAAnB,CAAuB,MAAvB;IACAT,UAAU,CAACU,KAAX,GANgB,CAMI;;IACpB,IAAIC,UAAS,GAAI,EAAjB;IACAP,aAAa,CAACQ,OAAd,CAAsBC,IAAG,IAAK;MAC5BF,UAAU,CAACG,IAAX,CAAgBD,IAAI,CAACE,WAArB;IACD,CAFD;;IAIA,MAAMC,aAAY,GAAI,MAAM;MAC1Bb,QAAQ,CAACK,SAAT,CAAmBS,MAAnB,CAA0B,MAA1B;IACF,CAFA;;IAIAjB,UAAU,CAACkB,gBAAX,CAA4B,OAA5B,EAAqC,MAAM;MACzCf,QAAQ,CAACK,SAAT,CAAmBC,GAAnB,CAAuB,MAAvB;MACA,IAAIU,UAAS,GAAInB,UAAU,CAACoB,KAAX,CAAiBC,WAAjB,EAAjB;MACA,IAAIC,cAAJ;;MACA,IAAIH,UAAU,CAACI,MAAX,GAAoB,CAAxB,EAA2B;QACzB,KAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAIb,UAAU,CAACY,MAA/B,EAAuCC,CAAC,EAAxC,EAA4C;UAC1C,IAAI,EAAEL,UAAU,CAACM,SAAX,CAAqB,CAArB,EAAwBN,UAAU,CAACI,MAAnC,MAA+CZ,UAAU,CAACa,CAAD,CAAV,CAAcC,SAAd,CAAwB,CAAxB,EAA2BN,UAAU,CAACI,MAAtC,EAA8CF,WAA9C,EAAjD,CAAJ,EAAmH;YACjHjB,aAAa,CAACoB,CAAD,CAAb,CAAiBhB,SAAjB,CAA2BC,GAA3B,CAA+B,QAA/B;UACF,CAFA,MAEO;YACLL,aAAa,CAACoB,CAAD,CAAb,CAAiBhB,SAAjB,CAA2BS,MAA3B,CAAkC,QAAlC;UACF;QACF;MACF,CARA,MAQO;QACL,KAAK,IAAIS,IAAI,CAAb,EAAgBA,IAAItB,aAAa,CAACmB,MAAlC,EAA0CG,CAAC,EAA3C,EAA+C;UAC7CtB,aAAa,CAACsB,CAAD,CAAb,CAAiBlB,SAAjB,CAA2BS,MAA3B,CAAkC,QAAlC;QACF;MACF;IACD,CAjBD;IAmBAb,aAAa,CAACQ,OAAd,CAAsBC,IAAG,IAAK;MAC5BA,IAAI,CAACK,gBAAL,CAAsB,OAAtB,EAAgCS,GAAD,IAAS;QACtC3B,UAAU,CAACoB,KAAX,GAAmBP,IAAI,CAACE,WAAxB;QACAX,aAAa,CAACQ,OAAd,CAAsBT,QAAO,IAAK;UAChCA,QAAQ,CAACK,SAAT,CAAmBC,GAAnB,CAAuB,QAAvB;QACD,CAFD;MAGD,CALD;IAMD,CAPD;IASAT,UAAU,CAACkB,gBAAX,CAA4B,OAA5B,EAAqC,MAAM;MACxClB,UAAU,CAAC4B,WAAX,GAAyB,gBAAzB;MACAzB,QAAQ,CAACK,SAAT,CAAmBC,GAAnB,CAAuB,MAAvB;MACAL,aAAa,