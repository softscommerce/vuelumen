{\r\n            try{\r\n                const datas = await axios.get('/api/user/work_station');\r\n                console.log(datas);\r\n                if(datas.data){\r\n                    data.value = datas.data;\r\n                }else{\r\n                    throw Error('Workstation data not found');\r\n                }\r\n               \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n         const getcategories = async (id) =>{\r\n            try{\r\n                const category = await axios.get(`/api/user/get_categories/${id}`);\r\n                console.log(category);\r\n                if(category.data){\r\n                    categories.value = category.data;\r\n                }else{\r\n                    throw Error('categories data not found');\r\n                }\r\n\r\n               \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n\r\n         const getBusinessType = async (id) =>{\r\n            try{\r\n                const getBusinessShopon = await axios.get(`/api/user/get_categories_shop/${id}`);\r\n                    console.log(getBusinessShopon.data);\r\n                if(getBusinessShopon.data){\r\n                    getCommunities()\r\n                }\r\n                    getBusinessShop.value = getBusinessShopon.data;\r\n              \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n\r\n         const getCommunities = async () =>{\r\n            try{\r\n                const getCommunity = await axios.get('/api/user/get_community');\r\n                console.log(getCommunity.data)\r\n                communities.value = getCommunity.data;\r\n              \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n\r\n\r\n\r\n\r\n        watch( select_change,\r\n            (currentValue, prevValue) => {\r\n             console.log(currentValue);\r\n             getcategories(currentValue);\r\n            },\r\n\r\n       );\r\n\r\n\r\n         watch( category,\r\n            (currentValue, prevValue) => {\r\n             getBusinessType(currentValue);\r\n            },\r\n\r\n       );\r\n\r\n      \r\n        // console.log(getBusinessType);\r\n        onMounted(()=>{\r\n            getworstations();\r\n            getmobiles();\r\n        });\r\n    \r\n\r\n\r\n        const createProfileSubmit = async ()=>{\r\n            const form = {\r\n                \r\n            }\r\n        }\r\n\r\n        return {\r\n            select_change,\r\n            data,\r\n            categories,\r\n            category,\r\n            get_mobile,\r\n            getBusinessShop,\r\n            communities,\r\n            community,\r\n            \r\n            \r\n        }\r\n    },\r\n\r\n    data(){\r\n        return{\r\n            form:{\r\n                address:'',\r\n                lat: '',\r\n                lng:'',\r\n\r\n            },\r\n            workstation:[]\r\n        }\r\n    },\r\n\r\n    methods:{\r\n\r\n        customerSelected(customer) {\r\n        console.log(`Customer Selected:\\nid: ${customer.id}\\nname: ${customer.name}`);\r\n        },\r\n        onChange(value) {\r\n        // do something with the current value\r\n        },\r\n        createProfileSubmit(){\r\n            console.log(this.$refs[\"origin\"].value);\r\n        },\r\n\r\n       async getWorkstation(){\r\n            try{\r\n                const data = await axios.get('/api/user/work_station');\r\n                if(data.data){\r\n                    this.workstation = data.data;\r\n                }else{\r\n                    throw Error('Workstation data not found');\r\n                }\r\n\r\n               \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n           \r\n        }\r\n    },\r\n\r\n    mounted(){\r\n        this.getWorkstation();\r\n        const originAutocomplete =  new google.maps.places.Autocomplete(\r\n            this.$refs[\"origin\"],\r\n            {\r\n                bounds: new google.maps.LatLngBounds(\r\n                    new google.maps.LatLng(45.2425296, -75.6971931)\r\n                )\r\n            }\r\n        );\r\n\r\n\r\n        originAutocomplete.addListener(\"place_changed\", () => {\r\n        const place = originAutocomplete.getPlace();\r\n        this.form.address = place.formatted_address;\r\n        this.form.lat = place.geometry.location.lat();\r\n        this.form.lng = place.geometry.location.lng();\r\n        console.log(place, this.form.address, this.form.lat, this.form.lng);\r\n\r\n       });\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.mb_2 { margin-bottom: 8px; }\r\n.mb_3 { margin-bottom: 12px; }\r\n.mb_4 { margin-bottom: 16px; }\r\n.d_flex { display: flex; column-gap: 20px; }\r\n.d_flex_10 { display: flex; column-gap: 10px; }\r\n.w_100 { width: 100%; }\r\n.service_page {\r\n    .service_setup {\r\n        .form_data {\r\n            width: 100%;\r\n            display: flex;\r\n            column-gap: 20px;\r\n            .left {\r\n                width: 400px;\r\n                margin: 0 auto;\r\n            }\r\n           \r\n        }\r\n    }\r\n}\r\n\r\n\r\n$phone-width-small: 360px;\r\n$phone-width: 575px;\r\n$tablet-width: 767px;\r\n$desktop-width: 1023px;\r\n\r\n@media (max-width: #{$desktop-width}) {\r\n    .service_page {\r\n        .service_setup {\r\n            .form_data {\r\n                .left {\r\n                    width: 280px;\r\n                }\r\n               \r\n            }\r\n        }\r\n    }\r\n}\r\n@media (max-width: #{$tablet-width}) {\r\n    .service_page {\r\n        .service_setup {\r\n            .form_data {\r\n                .left {\r\n                    width: 240px;\r\n                }\r\n             