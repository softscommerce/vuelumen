 (category.data) {\n          categories.value = category.data;\n        } else {\n          throw Error('categories data not found');\n        }\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    const getBusinessType = async id => {\n      try {\n        const getBusinessShopon = await axios.get(`/api/user/get_categories_shop/${id}`);\n\n        if (getBusinessShopon.data) {\n          getCommunities();\n        }\n\n        getBusinessShop.value = getBusinessShopon.data;\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    const getCommunities = async () => {\n      try {\n        const getCommunity = await axios.get('/api/user/get_community');\n        communities.value = getCommunity.data;\n      } catch (err) {\n        console.log(err.message);\n      }\n    }; //     watch( select_change,\n    //         (currentValue, prevValue) => {\n    //          getcategories(currentValue);\n    //         },\n    //    );\n    //  watch( category,\n    //     (currentValue, prevValue) => {\n    //      getBusinessType(currentValue);\n    //     },\n    //    );\n    // console.log(getBusinessType);\n\n\n    onMounted(() => {\n      getworstations();\n      getmobiles();\n      const originAutocomplete = new google.maps.places.Autocomplete(origin.value, {\n        bounds: new google.maps.LatLngBounds(new google.maps.LatLng(45.2425296, -75.6971931))\n      });\n      originAutocomplete.addListener(\"place_changed\", () => {\n        const place = originAutocomplete.getPlace();\n        location.value = place.formatted_address;\n        lat.value = place.geometry.location.lat();\n        lng.value = place.geometry.location.lng();\n      });\n    });\n\n    const customerSelected = customer => {\n      mobile.value = customer.mobile;\n    };\n\n    const createProfileSubmit = async () => {\n      const form = {\n        mobile: mobile.value,\n        workstation: select_change.value,\n        category: category.value,\n        community: community.value,\n        location: location.value,\n        lat: lat.value,\n        lng: lng.value,\n        business_name: business_name.value,\n        address: address.value,\n        paynow: paynow.value\n      };\n      axios.post('/api/user/user_service_profile_create', form).then(res => {\n        console.log(res);\n      });\n    };\n\n    return {\n      select_change,\n      data,\n      categories,\n      category,\n      get_mobile,\n      getBusinessShop,\n      communities,\n      community,\n      customerSelected,\n      origin,\n      createProfileSubmit,\n      business_name,\n      address,\n      paynow,\n      displayToKey,\n      displayToCategory,\n      displayToCommunity\n    };\n  }\n\n};","map":{"version":3,"mappings":";AAsIA,SAASA,GAAT,EAAcC,SAAd,EAAyBC,KAAzB,EAAgCC,QAAhC,QAAgD,KAAhD;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,aAAY,GAAIN,GAAG,CAAC,EAAD,CAAzB;IACA,MAAMO,QAAO,GAAIP,GAAG,CAAC,EAAD,CAApB;IACA,MAAMQ,MAAK,GAAIR,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMS,eAAc,GAAIT,GAAG,CAAC,KAAD,CAA3B;IACA,MAAMU,SAAQ,GAAIV,GAAG,CAAC,EAAD,CAArB;IACA,MAAMW,MAAK,GAAIX,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMY,QAAO,GAAIZ,GAAG,CAAC,EAAD,CAApB;IACA,MAAMa,GAAE,GAAIb,GAAG,CAAC,EAAD,CAAf;IACA,MAAMc,GAAE,GAAId,GAAG,CAAC,EAAD,CAAf;IACA,MAAMe,aAAY,GAAIf,GAAG,CAAC,EAAD,CAAzB;IACA,MAAMgB,OAAM,GAAIhB,GAAG,CAAC,EAAD,CAAnB;IACA,MAAMiB,MAAK,GAAIjB,GAAG,CAAC,QAAD,CAAlB;IAGA,MAAMkB,UAAS,GAAIlB,GAAG,CAAC,CAAC,EAAD,CAAD,CAAtB;IACC,MAAMmB,IAAG,GAAInB,GAAG,CAAC,EAAD,CAAhB;IACA,MAAMoB,UAAS,GAAIpB,GAAG,CAAC,EAAD,CAAtB;IACA,MAAMqB,WAAU,GAAIrB,GAAG,CAAC,EAAD,CAAvB;;IAGA,MAAMsB,YAAW,GAAMC,KAAD,IAAU;MAC7BC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACAjB,aAAa,CAACoB,KAAd,GAAsBH,KAAtB;;MACA,IAAGA,KAAI,IAAK,EAAZ,EAAe;QACfI,aAAa,CAACJ,KAAD,CAAb;MACA;IACA,CANH;;IAQA,MAAMK,iBAAgB,GAAML,KAAD,IAAU;MAClCC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACAhB,QAAQ,CAACmB,KAAT,GAAiBH,KAAjB;MACCM,eAAe,CAACN,KAAD,CAAf;MACAO,cAAc,CAACP,KAAD,CAAd;IACD,CALH;;IAOA,MAAMQ,kBAAiB,GAAMR,KAAD,IAAU;MAC