y=\"mobile\"\r\n                            @change=\"onChange\"\r\n                            title=\"Mobile Number\"\r\n                            @selected=\"customerSelected\"></autocomplete>\r\n                    </div>\r\n\r\n                    <div class=\"mb_3\">\r\n                        <base-select\r\n                            label=\"Work Station\"\r\n                            :options=\"data\"\r\n                            v-model=\"select_change\"\r\n                        ></base-select>\r\n                    </div>\r\n\r\n                    <div class=\"mb_3\">\r\n                        <base-select1\r\n                         label=\"Category\"\r\n                         :options=\"categories\"\r\n                          v-model=\"category\"\r\n                        >\r\n                        \r\n                        </base-select1>\r\n                    </div>\r\n\r\n<!-- v-if=\"getBusinessShop\" -->\r\n\r\n                    <div class=\"mb_3\"  v-if=\"getBusinessShop\">\r\n                        <base-select\r\n                         label=\"Community\"\r\n                         :options=\"communities\"\r\n                          v-model=\"community\"\r\n                        >\r\n\r\n                        </base-select>\r\n                    </div>\r\n                    <div class=\"mb_3\">\r\n                        <!-- <base-input\r\n                            label=\"Google Location\"\r\n                        v-model=\"origin\"></base-input> -->\r\n\r\n                        <input type=\"text\" name=\"\" style=\"height:40px; width:100%;\" ref=\"origin\" id=\"\">\r\n                    </div>\r\n                    <div class=\"mb_3 d_flex\">\r\n                        <base-radiobox\r\n                            divClass=\"d_flex_10\"\r\n                            label=\"Pay Now\"\r\n                            attributeName=\"true\"\r\n                        ></base-radiobox>\r\n                        <base-radiobox\r\n                            divClass=\"d_flex_10\"\r\n                            label=\"Trial\"\r\n                        ></base-radiobox>\r\n                    </div>\r\n                    <div class=\"mb_3\">\r\n                        <base-button\r\n                            small=\"btn_sm w_100 boder_radius1\"\r\n                            type=\"submit\"\r\n                        >Submit</base-button>\r\n                    </div>\r\n                </div>\r\n               \r\n            </form>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { ref, onMounted, watch, computed } from 'vue'\r\n\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    setup() {\r\n        const select_change = ref('');\r\n        const category = ref('');\r\n        const mobile = ref('');\r\n        const getBusinessShop = ref(false);\r\n        const community = ref('');\r\n        const origin = ref('');\r\n        const address = ref('');\r\n        const lat = ref('');\r\n        const lng = ref('');\r\n\r\n\r\n        const get_mobile = ref(['']);\r\n         const data = ref([]);\r\n         const categories = ref([]);\r\n         const communities = ref([]);\r\n         console.log(mobile)\r\n\r\n\r\n\r\n      const getmobiles = async () =>{\r\n            try{\r\n                const mobiles = await axios.get('/api/user/get_mobiles');\r\n                console.log(mobiles.data);\r\n                if(mobiles.data){\r\n                    get_mobile.value = mobiles.data;\r\n                }else{\r\n                    throw Error('User Mobile Number not found');\r\n                }\r\n\r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n        const getworstations = async () =>{\r\n            try{\r\n                const datas = await axios.get('/api/user/work_station');\r\n                console.log(datas);\r\n                if(datas.data){\r\n                    data.value = datas.data;\r\n                }else{\r\n                    throw Error('Workstation data not found');\r\n                }\r\n               \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n         const getcategories = async (id) =>{\r\n            try{\r\n                const category = await axios.get(`/api/user/get_categories/${id}`);\r\n                console.log(category);\r\n                if(category.data){\r\n                    categories.value = category.data;\r\n                }else{\r\n                    throw Error('categories data not found');\r\n                }\r\n\r\n               \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n\r\n         const getBusinessType = async (id) =>{\r\n            try{\r\n                const getBusinessShopon = await axios.get(`/api/user/get_categories_shop/${id}`);\r\n                    console.log(getBusinessShopon.data);\r\n                if(getBusinessShopon.data){\r\n                    getCommunities()\r\n                }\r\n                    getBusinessShop.value = getBusinessShopon.data;\r\n              \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n\r\n         const getCommunities = async () =>{\r\n            try{\r\n                const getCommunity = await axios.get('/api/user/get_community');\r\n                console.log(getCommunity.data)\r\n                communities.value = getCommunity.data;\r\n              \r\n            }catch(err){\r\n\r\n                console.log(err.message);\r\n            }\r\n        }\r\n\r\n\r\n\r\n\r\n\r\n        watch( select_change,\r\n            (currentValue, prevValue) => {\r\n             console.log(currentValue);\r\n             getcategories(currentValue);\r\n            },\r\n\r\n       );\r\n\r\n\r\n         watch( category,\r\n            (currentValue, prevValue) => {\r\n             getBusinessType(currentValue);\r\n            },\r\n\r\n       );\r\n\r\n      \r\n        // console.log(getBusinessType);\r\n        onMounted(()=>{\r\n            getworstations();\r\n            getmobiles();\r\n            console.log(origin.value)\r\n\r\n                const originAutocomplete =  new google.maps.places.Autocomplete(\r\n            origin.value,\r\n            {\r\n                bounds: new google.maps.LatLngBounds(\r\n                    new google.maps.LatLng(45.2425296, -75.6971931)\r\n                )\r\n            }\r\n        );\r\n\r\n\r\n         originAutocomplete.addListener(\"place_changed\", () => {\r\n        const place = originAutocomplete.getPlace();\r\n        address.value = place.formatted_address;\r\n        lat.value = place.geometry.location.lat();\r\n        lng.value = place.geometry.location.lng();\r\n        console.log(place);\r\n\r\n       });\r\n\r\n\r\n        });\r\n\r\n        const  customerSelected = (customer) => {\r\n            // console.log(`Customer Selected:\\nid: ${customer.id}\\nname: ${customer.mobile}`);\r\n            mobile.value = customer.mobile\r\n        }\r\n\r\n\r\n    \r\n\r\n\r\n        const createProfileSubmit = async ()=>{\r\n            const form = {\r\n                mobile: mobile.value,\r\n                workstation: select_change.value,\r\n                category: category.value,\r\n                community: community.value,\r\n                address:address.value,\r\n                lat: lat.value,\r\n                lng: lng.value\r\n\r\n            }\r\n\r\n\r\n            axios.post('/api/user/user_service_profile_create', form)\r\n            .then(res =>{\r\n                console.log('ok'+res);\r\n            })\r\n        }\r\n\r\n        return {\r\n            select_change,\r\n            data,\r\n            categories,\r\n            category,\r\n            get_mobile,\r\n            getBusinessShop,\r\n            communities,\r\n            community,\r\n            customerSelected,\r\n            origin,\r\n            createProfileSubmit\r\n            \r\n            \r\n        }\r\n    },\r\n\r\n   \r\n}\r\n</script>\r\n\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.mb_2 { margin-bottom: 8px; }\r\n.mb_3 { margin-bottom: 12px; }\r\n.mb_4 { margin-bottom: 16px; }\r\n.d_flex { display: flex; column-gap: 20px; }\r\n.d_flex_10 { display: flex; column-gap: 10px; }\r\n.w_100 { width: 100%; }\r\n.service_page {\r\n    .service_setup {\r\n        .form_data {\r\n            width: 100%;\r\n            display: flex;\r\n            column-gap: 20px;\r\n            .left {\r\n                width: 400px;\r\n                margin: 0 auto;\r\n            }\r\n           \r\n        }\r\n    }\r\n}\r\n\r\n\r\n$phone-width-small: 360px;\r\n$phone-width: 575px;\r\n$tablet-width: 767px;\r\n$desktop-width: 1023px;\r\n\r\n@media (max-width: #{$desktop-width}) {\r\n    .service_page {\r\n        .service_setup {\r\n            .form_data {\r\n                .left {\r\n                    width: 280px;\r\n                }\r\n               \r\n            }\r\n        }\r\n    }\r\n}\r\n@media (max-width: #{$tablet-width}) {\r\n    .service_page {\r\n        .service_setup {\r\n            .form_data {\r\n                .left {\r\n                    width: 240px;\r\n                }\r\n                \r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media (max-width: #{$phone-width}) {\r\n    .service_page {\r\n        .service_setup {\r\n            .form_data {\r\n                width: 100%;\r\n                display: block;\r\n                .left {\r\n                    width: 100%;\r\n                }\r\n               \r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media (max-width: #{$phone-width-small}) {\r\n    \r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}