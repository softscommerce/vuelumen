{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nimport { ref, onMounted, watch, computed } from 'vue';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const select_change = ref('');\n    const category = ref('');\n    const mobile = ref('');\n    const getBusinessShop = ref(false);\n    const community = ref('');\n    const origin = ref('');\n    const address = ref('');\n    const lat = ref('');\n    const lng = ref('');\n    const get_mobile = ref(['']);\n    const data = ref([]);\n    const categories = ref([]);\n    const communities = ref([]);\n    console.log(mobile);\n\n    const getmobiles = async () => {\n      try {\n        const mobiles = await axios.get('/api/user/get_mobiles');\n        console.log(mobiles.data);\n\n        if (mobiles.data) {\n          get_mobile.value = mobiles.data;\n        } else {\n          throw Error('User Mobile Number not found');\n        }\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    const getworstations = async () => {\n      try {\n        const datas = await axios.get('/api/user/work_station');\n        console.log(datas);\n\n        if (datas.data) {\n          data.value = datas.data;\n        } else {\n          throw Error('Workstation data not found');\n        }\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    const getcategories = async id => {\n      try {\n        const category = await axios.get(`/api/user/get_categories/${id}`);\n        console.log(category);\n\n        if (category.data) {\n          categories.value = category.data;\n        } else {\n          throw Error('categories data not found');\n        }\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    const getBusinessType = async id => {\n      try {\n        const getBusinessShopon = await axios.get(`/api/user/get_categories_shop/${id}`);\n        console.log(getBusinessShopon.data);\n\n        if (getBusinessShopon.data) {\n          getCommunities();\n        }\n\n        getBusinessShop.value = getBusinessShopon.data;\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    const getCommunities = async () => {\n      try {\n        const getCommunity = await axios.get('/api/user/get_community');\n        console.log(getCommunity.data);\n        communities.value = getCommunity.data;\n      } catch (err) {\n        console.log(err.message);\n      }\n    };\n\n    watch(select_change, (currentValue, prevValue) => {\n      console.log(currentValue);\n      getcategories(currentValue);\n    });\n    watch(category, (currentValue, prevValue) => {\n      getBusinessType(currentValue);\n    }); // console.log(getBusinessType);\n\n    onMounted(() => {\n      getworstations();\n      getmobiles();\n      console.log(origin.value);\n      const originAutocomplete = new google.maps.places.Autocomplete(origin.value, {\n        bounds: new google.maps.LatLngBounds(new google.maps.LatLng(45.2425296, -75.6971931))\n      });\n      originAutocomplete.addListener(\"place_changed\", () => {\n        const place = originAutocomplete.getPlace();\n        address.value = place.formatted_address;\n        lat.value = place.geometry.location.lat();\n        lng.value = place.geometry.location.lng();\n        console.log(place);\n      });\n    });\n\n    const customerSelected = customer => {\n      // console.log(`Customer Selected:\\nid: ${customer.id}\\nname: ${customer.mobile}`);\n      mobile.value = customer.mobile;\n    };\n\n    const createProfileSubmit = async () => {\n      const form = {\n        mobile: mobile.value,\n        workstation: select_change.value,\n        category: category.value,\n        community: community.value,\n        address: address.value,\n        lat: lat.value,\n        lng: lng.value\n      };\n      axios.post('/api/user/user_service_profile_create', form).then(res => {\n        console.log('ok' + res);\n      });\n    };\n\n    return {\n      select_change,\n      data,\n      categories,\n      category,\n      get_mobile,\n      getBusinessShop,\n      communities,\n      community,\n      customerSelected,\n      origin,\n      createProfileSubmit\n    };\n  }\n\n};","map":{"version":3,"mappings":";AAgFA,SAASA,GAAT,EAAcC,SAAd,EAAyBC,KAAzB,EAAgCC,QAAhC,QAAgD,KAAhD;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,aAAY,GAAIN,GAAG,CAAC,EAAD,CAAzB;IACA,MAAMO,QAAO,GAAIP,GAAG,CAAC,EAAD,CAApB;IACA,MAAMQ,MAAK,GAAIR,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMS,eAAc,GAAIT,GAAG,CAAC,KAAD,CAA3B;IACA,MAAMU,SAAQ,GAAIV,GAAG,CAAC,EAAD,CAArB;IACA,MAAMW,MAAK,GAAIX,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMY,OAAM,GAAIZ,GAAG,CAAC,EAAD,CAAnB;IACA,MAAMa,GAAE,GAAIb,GAAG,CAAC,EAAD,CAAf;IACA,MAAMc,GAAE,GAAId,GAAG,CAAC,EAAD,CAAf;IAGA,MAAMe,UAAS,GAAIf,GAAG,CAAC,CAAC,EAAD,CAAD,CAAtB;IACC,MAAMgB,IAAG,GAAIhB,GAAG,CAAC,EAAD,CAAhB;IACA,MAAMiB,UAAS,GAAIjB,GAAG,CAAC,EAAD,CAAtB;IACA,MAAMkB,WAAU,GAAIlB,GAAG,CAAC,EAAD,CAAvB;IACAmB,OAAO,CAACC,GAAR,CAAYZ,MAAZ;;IAIH,MAAMa,UAAS,GAAI,YAAW;MACxB,IAAG;QACC,MAAMC,OAAM,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CAAU,uBAAV,CAAtB;QACAJ,OAAO,CAACC,GAAR,CAAYE,OAAO,CAACN,IAApB;;QACA,IAAGM,OAAO,CAACN,IAAX,EAAgB;UACZD,UAAU,CAACS,KAAX,GAAmBF,OAAO,CAACN,IAA3B;QACH,CAFD,MAEK;UACD,MAAMS,KAAK,CAAC,8BAAD,CAAX;QACJ;MAEH,CATD,CASC,OAAMC,GAAN,EAAU;QAEPP,OAAO,CAACC,GAAR,CAAYM,GAAG,CAACC,OAAhB;MACJ;IACJ,CAdF;;IAgBE,MAAMC,cAAa,GAAI,YAAW;MAC9B,IAAG;QACC,MAAMC,KAAI,GAAI,MAAMzB,KAAK,CAACmB,GAAN,CAAU,wBAAV,CAApB;QACAJ,OAAO,CAACC,GAAR,CAAYS,KAAZ;;QACA,IAAGA,KAAK,CAACb,IAAT,EAAc;UACVA,IAAI,CAACQ,KAAL,GAAaK,KAAK,CAACb,IAAnB;QACH,CAFD,MAEK;UACD,MAAMS,KAAK,CAAC,4BAAD,CAAX;QACJ;MAEH,CATD,CASC,OAAMC,GAAN,EAAU;QAEPP,OAAO,CAACC,GAAR,CAAYM,GAAG,CAACC,OAAhB;MACJ;IACJ,CAdA;;IAgBC,MAAMG,aAAY,GAAI,MAAOC,EAAP,IAAa;MAChC,IAAG;QACC,MAAMxB,QAAO,GAAI,MAAMH,KAAK,CAACmB,GAAN,CAAW,4BAA2BQ,EAAG,EAAzC,CAAvB;QACAZ,OAAO,CAACC,GAAR,CAAYb,QAAZ;;QACA,IAAGA,QAAQ,CAACS,IAAZ,EAAiB;UACbC,UAAU,CAACO,KAAX,GAAmBjB,QAAQ,CAACS,IAA5B;QACH,CAFD,MAEK;UACD,MA