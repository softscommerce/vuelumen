{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport { ref, onMounted } from 'vue';\nimport productApi from './composible/product-api.js';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    const store = useStore();\n    const search_data = ref([]);\n    const search_text = ref('');\n    const products_data = ref([]);\n    const select_value = ref([]);\n    const selected_all = ref(false);\n    const checked_add = ref(false);\n    const product_action = ref('');\n\n    const selectAll = () => {\n      selected_all.value = !selected_all.valuel;\n      select_value.value = [];\n\n      if (selected_all.value) {\n        // Check all\n        for (var key in products_data.value) {\n          select_value.value.push(products_data.value[key]._id);\n        }\n      }\n    };\n\n    const updateCheckall = () => {\n      if (select_value.value == products_data.value) {\n        selected_all.value = true;\n      } else {\n        selected_all.value = false;\n      }\n    };\n\n    const productActionSubmit = async () => {\n      const form = new FormData();\n      form.append('select_value', select_value.value);\n      form.append('product_action', product_action.value);\n      console.log(select_value.value, selected_all.value);\n      const productDelete = await productApi.allProductDelete(select_value.value);\n    };\n\n    onMounted(async () => {\n      const products = await productApi.getAll();\n      products_data.value = products.data.data;\n      console.log(products_data.value);\n    });\n\n    const tagFetch = data => {\n      var items = '';\n      data.split(',').forEach(item => {\n        items += '#' + item + ' ';\n      });\n      return items;\n    };\n\n    const fetchDateTimeFormate = data => {\n      var items = data.split(' ');\n      return items[0] + ' at ' + items[1] + ' ' + items[2];\n    };\n\n    const deleteProduct = data => {\n      products_data.value = products_data.value.filter(item => {\n        return item._id != data._id;\n      });\n      productApi.delete(data._id).then(res => {\n        const payload = {\n          type: res.data.type,\n          message: res.data.data\n        };\n        store.dispatch('addNotification', payload);\n      });\n    };\n\n    const TableSearch = () => {\n      if (search_text.value.length > 0) {\n        return search_data.value = products_data.value.filter(item => {\n          return item.product_name.toLowerCase().includes(search_text.value.toLowerCase()) || item.category.name.toLowerCase().includes(search_text.value.toLowerCase()) || item.sale_price.toLowerCase().includes(search_text.value.toLowerCase());\n        });\n      } else {\n        return products_data.value;\n      }\n    };\n\n    return {\n      products_data,\n      search_data,\n      search_text,\n      TableSearch,\n      tagFetch,\n      fetchDateTimeFormate,\n      deleteProduct,\n      select_value,\n      selected_all,\n      selectAll,\n      checked_add,\n      product_action,\n      productActionSubmit,\n      updateCheckall\n    };\n  },\n\n  data() {\n    return {\n      isCheckAll: false,\n      langsdata: [\"PHP\", \"Vue.js\", \"AngularJS\", \"jQuery\", \"JavaScript\"],\n      languages: [],\n      selectedlang: \"\"\n    };\n  },\n\n  methods: {\n    checkAll: function () {\n      this.isCheckAll = !this.isCheckAll;\n      this.languages = [];\n\n      if (this.isCheckAll) {\n        // Check all\n        for (var key in this.langsdata) {\n          this.languages.push(this.langsdata[key]);\n        }\n      }\n    },\n    updateCheckall: function () {\n      if (this.languages.length == this.langsdata.length) {\n        this.isCheckAll = true;\n      } else {\n        this.isCheckAll = false;\n      }\n    },\n    printValues: function () {\n      this.selectedlang = \"\"; // Read Checked checkboxes value\n\n      for (var key in this.languages) {\n        this.selectedlang += this.languages[key] + \", \";\n      }\n    }\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":";AAiMA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EAEXC,KAAK,GAAG;IACL,MAAMC,KAAI,GAAIF,QAAQ,EAAtB;IACC,MAAMG,WAAU,GAAIN,GAAG,CAAC,EAAD,CAAvB;IACA,MAAMO,WAAU,GAAIP,GAAG,CAAC,EAAD,CAAvB;IACA,MAAMQ,aAAY,GAAIR,GAAG,CAAC,EAAD,CAAzB;IACA,MAAMS,YAAW,GAAIT,GAAG,CAAC,EAAD,CAAxB;IACA,MAAMU,YAAW,GAAIV,GAAG,CAAC,KAAD,CAAxB;IACA,MAAMW,WAAU,GAAIX,GAAG,CAAC,KAAD,CAAvB;IACA,MAAMY,cAAa,GAAIZ,GAAG,CAAC,EAAD,CAA1B;;IAEA,MAAMa,SAAQ,GAAI,MAAK;MAE3BH,YAAY,CAACI,KAAb,GAAqB,CAACJ,YAAY,CAACK,MAAnC;MACGN,YAAY,CAACK,KAAb,GAAqB,EAArB;;MACD,IAAGJ,YAAY,CAACI,KAAhB,EAAsB;QAAE;QACtB,KAAK,IAAIE,GAAT,IAAgBR,aAAa,CAACM,KAA9B,EAAqC;UACpCL,YAAY,CAACK,KAAb,CAAmBG,IAAnB,CAAyBT,aAAa,CAACM,KAAd,CAAoBE,GAApB,EAAyBE,GAAlD;QACD;MACF;IAEE,CAVA;;IAYD,MAAMC,cAAc,GAAE,MAAK;MACtB,IAAGV,YAAY,CAACK,KAAb,IAAsBN,aAAa,CAACM,KAAvC,EAA6C;QAC1CJ,YAAY,CAACI,KAAb,GAAqB,IAArB;MACF,CAFD,MAEK;QACDJ,YAAY,CAACI,KAAb,GAAqB,KAArB;MACJ;IACA,CANL;;IAQC,MAAMM,mBAAkB,GAAI,YAAU;MAClC,MAAMC,IAAG,GAAI,IAAIC,QAAJ,EAAb;MACAD,IAAI,CAACE,MAAL,CAAY,cAAZ,EAA4Bd,YAAY,CAACK,KAAzC;MACAO,IAAI,CAACE,MAAL,CAAY,gBAAZ,EAA8BX,cAAc,CAACE,KAA7C;MACAU,OAAO,CAACC,GAAR,CAAYhB,YAAY,CAACK,KAAzB,EAAgCJ,YAAY,CAACI,KAA7C;MACH,MAAMY,aAAY,GAAI,MAAMxB,UAAU,CAACyB,gBAAX,CAA4BlB,YAAY,CAACK,KAAzC,CAA5B;IACD,CANA;;IAQAb,SAAS,CAAC,YAAS;MAChB,MAAM2B,QAAO,GAAK,MAAM1B,UAAU,CAAC2B,MAAX,EAAxB;MACCrB,aAAa,CAACM,KAAd,GAAsBc,QAAQ,CAACE,IAAT,CAAcA,IAApC;MACAN,OAAO,CAACC,GAAR,CAAYjB,aAAa,CAACM,KAA1B;IACH,CAJQ,CAAT;;IAMA,MAAMiB,QAAO,GAAKD,IAAD,IAAS;MACtB,IAAIE,KAAI,GAAI,EAAZ;MACGF,IAAI,CAACG,KAAL,CAAW,GAAX,EAAgBC,OAAhB,CAAwBC,IAAG,IAAK;QAC/BH,KAAI,IAAK,MAAIG,IAAJ,GAAS,GAAlB;MACD,CAFA;MAGD,OAAOH,KAAP;IAEN,CAPA;;IAWA,MAAMI,oBAAmB,GAAMN,IAAF,IAAU;MAClC,IAAIE,KAAI,GAAIF,IAAI,CAACG,KAAL,CAAW,GAAX,CAAZ;MACC,OAAOD,KAAK,CAAC,CAAD,CAAL,GAAU,MAAV,GAAiBA,KAAK,CAAC,CAAD,CAAtB,GAA4B,GAA5B,GAAgCA,KAAK,CAAC,CAAD,CAA5C;IACN,CAHA;;IAKA,MAAMK,aAAY,GAAKP,IAAD,IAAS;MAC5BtB,aAAa,CAACM,KAAd,GAAsBN,aA