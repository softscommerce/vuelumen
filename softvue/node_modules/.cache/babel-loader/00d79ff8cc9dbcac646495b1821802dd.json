             <td class=\"in_stock\">In stock</td>\r\n                            <td class=\"price\">৳ 200.00</td>\r\n                            <td class=\"category\">Watch</td>\r\n                            <td class=\"tag\">---</td>\r\n                            <td class=\"star\">\r\n                                <img src=\"@/assets/icons/star.svg\" alt=\"Star\">\r\n                            </td>\r\n                            <td class=\"date_pub\">\r\n                                <p class=\"pub_unpub\">Published</p>\r\n                                <p class=\"date\">2020/06/20 at 4:38 am</p>\r\n                            </td>\r\n                            <td class=\"actions\">\r\n                                <router-link to=\"\" class=\"edit\">Edit</router-link>\r\n                                <router-link to=\"\" class=\"delete\">Delete</router-link>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n\r\n\r\n                    <tbody v-else>\r\n                        <tr v-for=\"(data, index) in products_data\" :key=\"index\">\r\n                            <td class=\"checkbox\">\r\n                                <base-checkbox></base-checkbox>\r\n                            </td>\r\n                            <td class=\"image\">\r\n                                <span><img style=\"width:75px; height:75px;\" :src=\"data.feature_image\" alt=\"Image\"></span>\r\n                            </td>\r\n                            <td class=\"name\">\r\n                                <a href=\"#\">{{ data.product_name }}</a>\r\n                            </td>\r\n                            <td class=\"sku\">---</td>\r\n                            <td class=\"in_stock\">{{data.units}}</td>\r\n                            <td class=\"price\">৳ {{data.sale_price}}</td>\r\n                            <td class=\"category\">Watch</td>\r\n                            <td class=\"tag\">{{t