\r\n        }\r\n\r\n        return {\r\n            ...toRefs(state),\r\n            onInput\r\n        }\r\n    }\r\n }   \r\n</script>\r\n<style lang=\"scss\" scoped>\r\n    .user_login {\r\n        font-family: Verdana,sans-serif;\r\n        height: 100%;\r\n        position: relative;\r\n    }\r\n    .header {\r\n        .navbar {\r\n            background: transparent;\r\n            display: flex;\r\n            align-items: center;\r\n            column-gap: 60px;\r\n            padding: 18px 26px;\r\n            .logo {\r\n                width: 40px;\r\n                height: 40px;\r\n                .home_link {\r\n                    display: block;\r\n                    img {\r\n                        max-width: 100%;\r\n                    }\r\n                }\r\n            }\r\n            .items{\r\n                display: flex;\r\n                align-items: center;\r\n                column-gap: 16px;\r\n                list-style: none;\r\n                .item {\r\n                    .link {\r\n                        font-size: 15px;\r\n                        color:#fff;\r\n                        padding: 8px;\r\n                        text-decoration: none;\r\n                        cursor: pointer;\r\n                        display: block;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n    .body {\r\n        height: 100%;\r\n        margin-top: -76px;\r\n        .top {\r\n            background: linear-gradient(306deg, #d61326 0, #161515 100%);\r\n            height: 429px;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: center;\r\n            h1 {\r\n                color: #fff;\r\n                font-size: 28px;\r\n                font-weight: 500;\r\n            }\r\n        }\r\n        .middle{\r\n            position: absolute;\r\n            width: 100%;\r\n            display: flex;\r\n            justify-content: center;\r\n      