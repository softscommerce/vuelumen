{"ast":null,"code":"// import { ref } from 'vue'\nimport { VueTelInput } from 'vue3-tel-input'; // import 'vue3-tel-input/dist/vue3-tel-input.css'\n\nimport axios from 'axios';\nimport { reactive, toRefs } from 'vue';\nexport default {\n  data() {\n    return {\n      phones: null,\n      countryCode: null,\n      register: false,\n      login: true,\n      user_check_in: false,\n      next: true,\n      signin: false,\n      form: {\n        mobile: null,\n        name: null,\n        passowrd: null,\n        passowrdCon: null\n      }\n    };\n  },\n\n  created() {\n    if (localStorage.getItem('mobile')) {\n      this.register = true;\n      this.login = false;\n      this.form.mobile = localStorage.getItem('mobile'); // document.getElementById(\"mobile\").setAttribute(\"disabled\", false);\n    } else {\n      this.register = false;\n      this.login = true;\n    }\n  },\n\n  components: {\n    VueTelInput\n  },\n  methods: {\n    async loginSubmit() {\n      const form = {\n        mobile: this.phone,\n        password: this.password\n      };\n      const login = await axios.post('/api/login', form);\n      console.log(login);\n    },\n\n    destroyLocalStorage() {\n      localStorage.removeItem('mobile');\n      this.register = false;\n      this.login = true;\n    },\n\n    async nextButton() {\n      if (this.countryCode == '880') {\n        console.log(this.phone);\n        let phone = {\n          phone: this.phone\n        };\n        let userCheck = await axios.post('/api/get_users', phone);\n\n        if (userCheck.data) {\n          this.user_check_in = true;\n          this.next = false;\n          this.signin = true; //    document.getElementById(\"signin\").innerText = \"Sign In\";\n        } else {\n          this.register = true;\n          this.login = false;\n          localStorage.setItem('mobile', this.phone);\n          this.form.mobile = localStorage.getItem('mobile');\n        }\n      } else {\n        alert('Please Select Bangladesh Country');\n      }\n    },\n\n    countryChanged(country) {\n      this.countryCode = country.dialCode;\n      console.log(this.countryCode);\n    }\n\n  },\n  setup: () => {\n    const state = reactive({\n      phone: null\n    });\n\n    const onInput = (phone, phoneObject, input) => {\n      if (phoneObject?.formatted) {\n        state.phone = phoneObject.formatted;\n      }\n    };\n\n    return { ...toRefs(state),\n      onInput\n    };\n  }\n};","map":{"version":3,"mappings":"AAyJA;AACA,SAASA,WAAT,QAA4B,gBAA5B,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,EAAmBC,MAAnB,QAAiC,KAAjC;AAEC,eAAc;EACXC,IAAI,GAAE;IACF,OAAM;MACFC,MAAM,EAAE,IADN;MAEFC,WAAW,EAAE,IAFX;MAGFC,QAAQ,EAAC,KAHP;MAIFC,KAAK,EAAC,IAJJ;MAKFC,aAAa,EAAC,KALZ;MAMFC,IAAI,EAAC,IANH;MAOFC,MAAM,EAAC,KAPL;MASFC,IAAI,EAAC;QACDC,MAAM,EAAC,IADN;QAEDC,IAAI,EAAC,IAFJ;QAGDC,QAAQ,EAAC,IAHR;QAIDC,WAAW,EAAC;MAJX;IATH,CAAN;EAiBH,CAnBU;;EAqBXC,OAAO,GAAE;IACL,IAAGC,YAAY,CAACC,OAAb,CAAqB,QAArB,CAAH,EAAkC;MAC9B,KAAKZ,QAAL,GAAgB,IAAhB;MACA,KAAKC,KAAL,GAAa,KAAb;MACA,KAAKI,IAAL,CAAUC,MAAV,GAAmBK,YAAY,CAACC,OAAb,CAAqB,QAArB,CAAnB,CAH8B,CAI9B;IACH,CALD,MAKK;MACA,KAAKZ,QAAL,GAAgB,KAAhB;MACA,KAAKC,KAAL,GAAa,IAAb;IACL;EACH,CA/BU;;EAmCZY,UAAU,EAAE;IACXpB;EADW,CAnCA;EAuCVqB,OAAO,EAAE;IACP,MAAMC,WAAN,GAAmB;MAClB,MAAMV,IAAG,GAAG;QACRC,MAAM,EAAE,KAAKU,KADL;QAERC,QAAQ,EAAE,KAAKA;MAFP,CAAZ;MAII,MAAMhB,KAAI,GAAI,MAAMP,KAAK,CAACwB,IAAN,CAAW,YAAX,EAAyBb,IAAzB,CAApB;MACAc,OAAO,CAACC,GAAR,CAAYnB,KAAZ;IAEH,CATK;;IAWNoB,mBAAmB,GAAE;MACjBV,YAAY,CAACW,UAAb,CAAwB,QAAxB;MACA,KAAKtB,QAAL,GAAgB,KAAhB;MACA,KAAKC,KAAL,GAAa,IAAb;IACH,CAfK;;IAgBP,MAAMsB,UAAN,GAAmB;MACd,IAAG,KAAKxB,WAAL,IAAoB,KAAvB,EAA6B;QACzBoB,OAAO,CAACC,GAAR,CAAY,KAAKJ,KAAjB;QAEC,IAAIA,KAAI,GAAI;UACTA,KAAK,EAAE,KAAKA;QADH,CAAZ;QAIF,IAAIQ,SAAQ,GAAI,MAAM9B,KAAK,CAACwB,IAAN,CAAW,gBAAX,EAA4BF,KAA5B,CAAtB;;QAEA,IAAGQ,SAAS,CAAC3B,IAAb,EAAkB;UAClB,KAAKK,aAAL,GAAqB,IAArB;UACA,KAAKC,IAAL,GAAY,KAAZ;UACA,KAAKC,MAAL,GAAc,IAAd,CAHkB,CAIrB;QAEI,CAND,MAMK;UACJ,KAAKJ,QAAL,GAAgB,IAAhB;UACA,KAAKC,